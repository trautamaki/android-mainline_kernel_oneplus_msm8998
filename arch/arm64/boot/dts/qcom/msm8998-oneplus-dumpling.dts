#include "msm8998-oneplus-common.dtsi"

/ {
	model = "OnePlus 5T";
	compatible = "qcom,msm8998-mtp\0qcom,msm8998\0qcom,mtp";
	/* Required for bootloader to select correct board */
	qcom,msm-id = <0x124 0x20001>; /* 8998 v2.1 */
	qcom,board-id = <8 0 17801 43>; // 53, 43?
	// FIXME: Regardless of above, appending both model DTBs doesn't boot on 5T!
};

/* Adjust digitizer area height to match the 5T's taller panel */
&rmi4_f12 {
	touchscreen-y-mm = <137>;
};
